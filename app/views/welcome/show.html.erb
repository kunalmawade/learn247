<% if current_user.student? %>
	<%= render partial: 'student_home', local:{ courses: @courses} %>
<% elsif current_user.instructor? %>
	<%= render partial: 'instructor_home', local:{ courses: @courses} %>
<% elsif current_user.admin? %>
	<%= render partial: 'admin_home', local:{ courses: @courses} %>
<% else %>
	<%= render partial: 'guest_home' %>
<% end %>